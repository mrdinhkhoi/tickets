<p id="notice"><%= notice %></p>

<h1>Listing Ticket Attachments</h1>

<table>
  <thead>
    <tr>
      <th>Ticket</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticket_attachments.each do |ticket_attachment| %>
      <tr>
        <td><%= ticket_attachment.ticket_id %></td>
        <td><%= ticket_attachment.image %></td>
        <td><%= link_to 'Show', ticket_attachment %></td>
        <td><%= link_to 'Edit', edit_ticket_attachment_path(ticket_attachment) %></td>
        <td><%= link_to 'Destroy', ticket_attachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket attachment', new_ticket_attachment_path %>
